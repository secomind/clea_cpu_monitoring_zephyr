{
    "interface_name": "com.example.poc.CpuMetrics",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "individual",
    "mappings": [
      {
        "endpoint": "/loadavg",
        "type": "double",
        "database_retention_policy": "use_ttl",
        "database_retention_ttl": 5184000,
        "explicit_timestamp": true,
        "description": "CPU load averaged over 1 minute"
      }
    ]
  }
